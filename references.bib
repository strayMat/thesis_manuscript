@article{malafaye_mise_2018,
  title        = {Mise en place et exploitation d’un entrepôt de données
                  au département d’information médicale du {CHU} de
                  Montpellier, France},
  volume       = {66},
  series       = {Colloque Adelf-Emois - Montpellier, 29 et 30 mars 2018},
  abstract     = {Introduction Les missions du département d’information
                  médicale ({DIM}) ne se limitent plus à la production du
                  {PMSI} de l’exercice en cours. La mise à disposition
                  d’indicateurs d’aide à la décision est essentielle au
                  pilotage d’un établissement. Elle nécessite
                  l’interrogation de données issues de sources multiples
                  et sur plusieurs années. Pour répondre à cet objectif,
                  le {DIM} du {CHU} de Montpellier a créé un entrepôt de
                  données (entrepôt) indépendant du {SIH} de production.
                  Cet entrepôt assure la centralisation et l’historisation
                  des données issues des différents champs et domaines
                  d’activité de l’hôpital. Méthodes L’entrepôt est
                  une base de données relationnelle. Les données sont
                  consolidées depuis 1996. Concernant l’exercice en cours,
                  un ensemble de programme alimente quotidiennement
                  l’entrepôt. Lors de cette phase, des contrôles
                  permettent de vérifier l’intégrité des données et
                  d’améliorer la qualité des informations collectées.
                  Résultats À ce jour, l’entrepôt regroupe 323 tables
                  concernant 13,5 millions de séjours et 2,2 millions de
                  patients. Les informations sont issues notamment des
                  domaines suivants : {PMSI}, mouvements, actes,
                  médicaments, dispositifs médicaux implantables,
                  facturation, comptes rendus médicaux. Les dossiers de
                  spécialités représentent le plus gros volume de données
                  avec 22,3 millions de questionnaires et 186,4 millions de
                  réponses. L’analyse croisée de ces données issues de
                  sources multiples a permis de créer de nouveaux
                  indicateurs (indicateurs d’exploitation des blocs
                  chirurgicaux…) ou de faciliter le recueil d’indicateurs
                  ({IPAQSS} « Traçabilité de l’évaluation de la
                  douleur »…). L’exploitation de données hors du champ
                  {PMSI} (biologie, {IMC}, transmissions infirmières…)
                  contribue à l’optimisation des recettes T2A.
                  Discussion/Conclusion Cet entrepôt est évolutif et
                  garantit la pérennité des données. Il a permis au {DIM}
                  d’améliorer le traitement des informations et de mettre
                  à disposition des services (Direction, Recherche clinique,
                  services médicaux…) des indicateurs de pilotage
                  élaborés, notamment le suivi de réalisation des contrats
                  de pôle développé en collaboration avec le Contrôle de
                  gestion. L’adaptation de cet entrepôt au nouveau
                  dispositif des {GHT} constituera un des enjeux principaux
                  des prochaines années.},
  pages        = {S26},
  journal      = {Revue d'Épidémiologie et de Santé Publique},
  shortjournal = {Revue d'Épidémiologie et de Santé Publique},
  author       = {Malafaye, N. and Demoulin, D. and Mailhe, P. and Morell,
                  M. and Pellecuer, D. and Dunoyer, C.},
  date         = {2018-03-01},
  year         = {2018}
}

